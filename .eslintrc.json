{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "react", "jsx-a11y", "import"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  }
}
